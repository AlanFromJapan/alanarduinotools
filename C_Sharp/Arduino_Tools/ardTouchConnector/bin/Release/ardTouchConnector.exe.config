<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Default COM port to select in the list -->
		<add key="COMPort" value="COM18" />

		<!-- On startup start directly to listen ?-->
		<add key="ListenOnStartup" value="true" />

    <!-- On startup start fetching mail info from outlook ?-->
    <add key="FetchMailStatusOnStartup" value="true" />

    <!-- Shall the window start minimized and hide in toolbar ?-->
		<add key="MinimizeOnStartup" value="false" />
	</appSettings>
</configuration>