			<customRun label="APPCOMMAND_VOLUME_UP" key="V" command="10" params="" mustConfirm="false" />
			<customRun label="APPCOMMAND_MEDIA_NEXTTRACK" key="&gt;" command="11" params="" mustConfirm="false" />
			<customRun label="APPCOMMAND_MEDIA_PREVIOUSTRACK" key="&lt;" command="12" params="" mustConfirm="false" />
		</mediaPlayer>
	</commandRunInterpreter>
